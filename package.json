{
  "name": "kiwimeri-app",
  "description": "A Pleasant Writing App",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"node middleware.cjs\" \"ionic serve\"",
    "start-middleware": "node middleware.cjs",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "lint": "eslint",
    "format": "prettier --write \"src/\"",
    "i18n": "lingui extract --clean && lingui compile --typescript",
    "release:docker": "node scripts/release-docker.cjs",
    "release:electron": "node --env-file=.env.local scripts/release-electron.cjs"
  },
  "dependencies": {
    "@capacitor-community/electron": "^5.0.1",
    "@capacitor/android": "7.0.1",
    "@capacitor/app": "7.0.0",
    "@capacitor/core": "7.0.1",
    "@capacitor/filesystem": "^7.0.0",
    "@capacitor/haptics": "7.0.0",
    "@capacitor/keyboard": "7.0.0",
    "@capacitor/status-bar": "7.0.0",
    "@ionic/react": "^8.0.0",
    "@ionic/react-router": "^8.0.0",
    "@lexical/react": "^0.27.2",
    "@lingui/react": "^5.2.0",
    "@types/node": "^22.13.9",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "ionicons": "^7.0.0",
    "lexical": "^0.27.2",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "tinybase": "^5.3.0"
  },
  "devDependencies": {
    "@capacitor/cli": "7.0.1",
    "@ionic/cli": "^7.2.0",
    "@lingui/babel-plugin-lingui-macro": "^5.2.0",
    "@lingui/cli": "^5.2.0",
    "@lingui/vite-plugin": "^5.2.0",
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "concurrently": "^9.1.2",
    "cors-anywhere": "^0.4.4",
    "cypress": "^13.5.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^22.1.0",
    "prettier": "^3.5.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "sass": "^1.86.3",
    "terser": "^5.4.0",
    "typescript": "^5.1.6",
    "vite": "~5.2.0",
    "vitest": "^0.34.6"
  }
}
