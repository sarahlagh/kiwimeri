{
  "zipName": "SimpleLayer.zip",
  "testCases": [
    {
      "description": "with no init data",
      "initData": [],
      "scenarios": [
        {
          "description": "and removeFirstFolder=true and createNotebook=false",
          "options": [
            {
              "removeFirstFolder": true,
              "createNotebook": false,
              "createNewFolder": true
            }
          ],
          "expected": {
            "newItems": [
              {
                "id": "#id",
                "title": "New folder",
                "parent": "0",
                "type": "f",
                "created": 1753088490685,
                "updated": 1753088490685
              },
              {
                "title": "Simple",
                "parent": "#id",
                "type": "d",
                "content": "__defined__",
                "created": 1753088495423,
                "updated": 1753088495423
              }
            ],
            "firstLevel": [
              {
                "title": "New folder",
                "type": "f",
                "status": "new"
              }
            ]
          }
        },
        {
          "description": "and removeFirstFolder=true and createNewFolder=true",
          "options": [
            {
              "removeFirstFolder": true,
              "createNewFolder": true
            }
          ],
          "expected": {
            "newItems": [
              {
                "id": "#id",
                "title": "New folder",
                "parent": "0",
                "created": 1753088490685,
                "updated": 1753088490685,
                "type": "f"
              },
              {
                "title": "Simple",
                "parent": "#id",
                "type": "d",
                "content": "__defined__",
                "created": 1753088495423,
                "updated": 1753088495423
              }
            ],
            "firstLevel": [
              {
                "title": "New folder",
                "type": "f",
                "status": "new"
              }
            ]
          }
        },
        {
          "description": "and createNotebook=true",
          "options": [
            {
              "createNotebook": true,
              "removeFirstFolder": false,
              "createNewFolder": true
            },
            {
              "createNotebook": true,
              "removeFirstFolder": false,
              "createNewFolder": false
            },
            {
              "createNotebook": true,
              "removeFirstFolder": true,
              "createNewFolder": true
            },
            {
              "createNotebook": true,
              "removeFirstFolder": true,
              "createNewFolder": false
            }
          ],
          "expected": {
            "newItems": [
              {
                "id": "#notebook",
                "title": "New folder",
                "type": "n",
                "created": 1753088490685,
                "updated": 1753088490685
              },
              {
                "id": "#id2",
                "title": "Simple Original",
                "parent": "#notebook",
                "type": "f",
                "created": 1753088495423,
                "updated": 1753088495423
              },
              {
                "title": "Simple",
                "parent": "#id2",
                "type": "d",
                "content": "__defined__",
                "created": 1753088495423,
                "updated": 1753088495423
              }
            ],
            "firstLevel": [
              {
                "title": "New folder",
                "type": "n",
                "status": "new"
              }
            ]
          }
        }
      ]
    }
  ]
}
