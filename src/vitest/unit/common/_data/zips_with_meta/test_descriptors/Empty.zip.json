{
  "zipName": "Empty.zip",
  "name": "Empty",
  "testCases": [
    {
      "description": "with no init data",
      "initData": [],
      "scenarios": [
        {
          "description": "with createNotebook=false (createNewFolder is ignored)",
          "options": [
            {
              "createNotebook": false,
              "createNewFolder": true,
              "overwrite": false
            },
            {
              "createNotebook": false,
              "createNewFolder": true,
              "overwrite": false
            },
            {
              "createNotebook": false,
              "createNewFolder": false,
              "overwrite": false
            }
          ],
          "expected": {
            "newItems": [
              {
                "title": "empty",
                "parent": "0",
                "type": "f",
                "created": 1752772097139,
                "updated": 1752772101238
              }
            ],
            "firstLevel": [
              {
                "title": "empty",
                "type": "f",
                "status": "new"
              }
            ]
          }
        },
        {
          "description": "with createNotebook=false and createNewFolder=NewName",
          "options": [
            {
              "createNotebook": false,
              "createNewFolder": true,
              "newFolderName": "NewName",
              "overwrite": false
            }
          ],
          "expected": {
            "newItems": [
              {
                "title": "NewName",
                "parent": "0",
                "type": "f",
                "created": 1752772097139,
                "updated": 1752772101238
              }
            ],
            "firstLevel": [
              {
                "title": "NewName",
                "status": "new",
                "type": "f"
              }
            ]
          }
        },
        {
          "description": "with createNotebook=true (createNewFolder is ignored)",
          "options": [
            {
              "createNotebook": true,
              "createNewFolder": true,
              "overwrite": false
            },
            {
              "createNotebook": true,
              "createNewFolder": true,
              "overwrite": false
            },
            {
              "createNotebook": true,
              "createNewFolder": false,
              "overwrite": false
            }
          ],
          "expected": {
            "newItems": [
              {
                "title": "empty",
                "parent": "0",
                "type": "n",
                "created": 1752772097139,
                "updated": 1752772101238
              }
            ],
            "firstLevel": [
              {
                "title": "empty",
                "type": "n",
                "status": "new"
              }
            ]
          }
        },
        {
          "description": "with createNotebook=true and createNewFolder=NewName",
          "options": [
            {
              "createNotebook": true,
              "createNewFolder": true,
              "newFolderName": "NewName",
              "overwrite": false
            },
            {
              "createNotebook": true,
              "createNewFolder": false,
              "newFolderName": "NewName",
              "overwrite": false
            }
          ],
          "expected": {
            "newItems": [
              {
                "title": "NewName",
                "parent": "0",
                "type": "n",
                "created": 1752772097139,
                "updated": 1752772101238
              }
            ],
            "firstLevel": [
              {
                "title": "NewName",
                "status": "new",
                "type": "n"
              }
            ]
          }
        },
        {
          "description": "with ignoreMetadata=true",
          "options": [
            {
              "ignoreMetadata": true,
              "createNewFolder": false,
              "overwrite": false
            }
          ],
          "expected": {
            "newItems": [
              {
                "title": "meta.json",
                "parent": "0",
                "type": "d"
              }
            ],
            "firstLevel": [
              {
                "title": "meta.json",
                "type": "d",
                "status": "new"
              }
            ]
          }
        }
      ]
    }
  ]
}
