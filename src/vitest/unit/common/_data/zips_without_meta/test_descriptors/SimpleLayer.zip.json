{
  "zipName": "SimpleLayer.zip",
  "testCases": [
    {
      "description": "with no init data",
      "initData": [],
      "scenarios": [
        {
          "description": "and removeFirstFolder=true and createNewFolder=true",
          "options": [
            {
              "removeFirstFolder": true,
              "createNewFolder": true,
              "newFolderName": "Simple",
              "overwrite": false
            },
            {
              "removeFirstFolder": true,
              "createNewFolder": true,
              "newFolderName": "Simple",
              "overwrite": true
            }
          ],
          "expected": {
            "newItems": [
              {
                "id": "#id",
                "title": "Simple",
                "parent": "0",
                "type": "f"
              },
              {
                "title": "Simple",
                "parent": "#id",
                "type": "d",
                "content": "__defined__"
              }
            ],
            "firstLevel": [
              {
                "title": "Simple",
                "type": "f",
                "status": "new"
              }
            ]
          }
        },
        {
          "description": "and removeFirstFolder=true and createNewFolder=false",
          "options": [
            {
              "removeFirstFolder": true,
              "createNewFolder": false,
              "overwrite": false
            },
            {
              "removeFirstFolder": true,
              "createNewFolder": false,
              "overwrite": true
            }
          ],
          "expected": {
            "newItems": [
              {
                "title": "Simple",
                "parent": "0",
                "type": "d",
                "content": "__defined__"
              }
            ],
            "firstLevel": [
              {
                "title": "Simple",
                "type": "d",
                "status": "new"
              }
            ]
          }
        },
        {
          "description": "and removeFirstFolder=false and createNewFolder=true",
          "options": [
            {
              "removeFirstFolder": false,
              "createNewFolder": true,
              "overwrite": false
            },
            {
              "removeFirstFolder": false,
              "createNewFolder": true,
              "overwrite": true
            }
          ],
          "expected": {
            "newItems": [
              {
                "id": "#id1",
                "title": "SimpleLayer",
                "parent": "0",
                "type": "f"
              },
              {
                "id": "#id2",
                "title": "Simple",
                "parent": "#id1",
                "type": "f"
              },
              {
                "title": "Simple",
                "parent": "#id2",
                "type": "d",
                "content": "__defined__"
              }
            ],
            "firstLevel": [
              {
                "title": "SimpleLayer",
                "type": "f",
                "status": "new"
              }
            ]
          }
        },
        {
          "description": "and removeFirstFolder=false and createNewFolder=false",
          "options": [
            {
              "removeFirstFolder": false,
              "createNewFolder": false,
              "overwrite": false
            },
            {
              "removeFirstFolder": false,
              "createNewFolder": false,
              "overwrite": true
            }
          ],
          "expected": {
            "newItems": [
              {
                "id": "#id",
                "title": "Simple",
                "parent": "0",
                "type": "f"
              },
              {
                "title": "Simple",
                "parent": "#id",
                "type": "d",
                "content": "__defined__"
              }
            ],
            "firstLevel": [
              {
                "title": "Simple",
                "type": "f",
                "status": "new"
              }
            ]
          }
        },
        {
          "description": "and createNotebook=true and overwrite=false",
          "mergeInto": "root",
          "options": [
            {
              "createNotebook": true,
              "removeFirstFolder": true,
              "createNewFolder": true,
              "newFolderName": "Default",
              "overwrite": false
            },
            {
              "createNotebook": true,
              "removeFirstFolder": false,
              "createNewFolder": false,
              "newFolderName": "Default",
              "overwrite": false
            }
          ],
          "expected": {
            "newItems": [
              {
                "id": "#notebook",
                "title": "Default",
                "parent": "root",
                "type": "n"
              },
              {
                "id": "#id2",
                "title": "Simple",
                "parent": "#notebook",
                "type": "f"
              },
              {
                "title": "Simple",
                "parent": "#id2",
                "type": "d",
                "content": "__defined__"
              }
            ],
            "firstLevel": [
              {
                "title": "Default",
                "type": "n",
                "status": "new"
              }
            ],
            "duplicates": [
              {
                "id": "0",
                "title": "Default",
                "type": "n"
              }
            ]
          }
        },
        {
          "description": "and createNotebook=true and overwrite=true",
          "mergeInto": "root",
          "options": [
            {
              "createNotebook": true,
              "removeFirstFolder": true,
              "createNewFolder": true,
              "newFolderName": "Default",
              "overwrite": true
            },
            {
              "createNotebook": true,
              "removeFirstFolder": false,
              "createNewFolder": false,
              "newFolderName": "Default",
              "overwrite": true
            }
          ],
          "expected": {
            "updatedItems": [
              {
                "id": "0",
                "title": "Default",
                "parent": "root",
                "type": "n",
                "created": -1
              }
            ],
            "newItems": [
              {
                "id": "#id2",
                "title": "Simple",
                "parent": "0",
                "type": "f"
              },
              {
                "title": "Simple",
                "parent": "#id2",
                "type": "d",
                "content": "__defined__"
              }
            ],
            "firstLevel": [
              {
                "title": "Default",
                "type": "n",
                "status": "merged"
              }
            ],
            "duplicates": [
              {
                "id": "0",
                "title": "Default",
                "type": "n"
              }
            ]
          }
        }
      ]
    },
