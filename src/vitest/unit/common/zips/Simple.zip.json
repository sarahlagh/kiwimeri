          "description": "and createNewFolder=true",
          "options": [
            {
              "createNewFolder": true,
              "overwrite": false
            },
            {
              "createNewFolder": true,
              "overwrite": true
            }
          ],
          "expected": {
            "newItems": [
              {
                "id": "#id",
                "title": "Simple",
                "parent": "home",
                "type": "f"
              },
              {
                "title": "Simple",
                "parent": "#id",
                "type": "d",
                "content": "__defined__"
              }
            ],
            "firstLevel": [
              {
                "title": "Simple",
                "type": "f",
                "status": "new"
              }
            ]
          }
        },
        {
